                                "name": "$expand",
                                "value": ""
                              },
                              {
                                "name": "$count",
                                "value": "event.top != null && event.skip != null"
                              },
                              {
                                "name": "$orderby",
                                "value": "${event.orderby}"
                              }
                            ],
