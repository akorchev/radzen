  "events": {
    "load": [
      {
        "name": "totalSales",
        "type": "set",
        "value": "49234"
      },
      {
        "name": "salesTarget",
        "type": "set",
        "value": "80000"
      },
      {
        "name": "customers",
        "type": "set",
        "value": "[134, 193, 156, 123, 192, 155, 132, 163, 143, 182, 192, 233, 129, 183, 233, 150, 113, 233, 323, 354, 433, 425, 365, 322, 280, 321, 393, 436,530,553,576]"
      },
      {
        "name": "employeeOfTheMonthName",
        "type": "set",
        "value": "\"Loading ...\""
      },
      {
        "name": "Northwind.getEmployeeById",
        "parameters": [
          {
            "name": "EmployeeID",
            "value": "1"
          }
        ],
        "then": [
          {
            "name": "employeeOfTheMonth",
            "type": "set",
            "value": "${result}"
          },
          {
            "name": "employeeOfTheMonthName",
            "type": "set",
            "value": "${result.FirstName} + ' ' + ${result.LastName}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Northwind.getEmployees",
        "parameters": [
          {
            "name": "$top",
            "value": "this.grid0.allowPaging ? this.grid0.pageSize : null"
          },
          {
            "name": "$skip",
            "value": "this.grid0.allowPaging ? 0 : null"
          },
          {
            "name": "$count",
            "value": "this.grid0.allowPaging"
          }
        ],
        "then": [
          {
            "name": "getEmployeesResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getEmployeesCount",
            "type": "set",
            "value": "this.grid0.allowPaging ? result['@odata.count'] : ${result.value.length}"
          },
          {
            "code": "this.grid0.isLoading = false",
            "type": "execute"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "Dashboard"
